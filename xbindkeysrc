# record new key by: xbindkeys -k

"amixer -D pulse set Master 1%+;alsactl --file ~/.config/asound.state store"
    m:0x0 + c:123
    XF86AudioRaiseVolume
"amixer -D pulse set Master 1%-;alsactl --file ~/.config/asound.state store"
    m:0x0 + c:122
    XF86AudioLowerVolume
"amixer -D pulse set Master 0%;"
    m:0x0 + c:121
    XF86AudioMute

